{
  "name": "ai-platform-tools",
  "version": "1.1.0",
  "description": "AI Platform Development Tools",
  "main": "index.js",
  "engines": {
    "vscode": "^1.60.0"
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.2",
    "@types/ajv": "^1.0.4",
    "@types/express": "^5.0.0",
    "ajv": "^8.17.1",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/node": "^16.18.122",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.9.5"
  },
  "activationEvents": [
    "onCommand:aiPlatform.makeAPICall",
    "onCommand:aiPlatform.batchProcess"
  ],
  "contributes": {
    "commands": [
      {
        "command": "aiPlatform.makeAPICall",
        "title": "AI Platform: Make API Call"
      },
      {
        "command": "aiPlatform.batchProcess",
        "title": "AI Platform: Batch Process"
      }
    ]
  }
}
